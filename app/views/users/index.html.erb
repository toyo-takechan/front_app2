<% provide(:title, 'All users') %>
<div class="wrapper">
  <%= render 'layouts/aside' %>
  <div class="wrapper_main " >
   <%= will_paginate %>
    <ul class="users">
      <% @users.each do |user| %>
        <li>
          <%= gravatar_for user, size: 50 %>
          <%= link_to user.name, user_path(user) %>
        </li>
      <% end %>
    </ul>
    <%= will_paginate %>
  </div>
</div>